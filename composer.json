{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "fakerphp/faker": "^1.23",
        "giggsey/libphonenumber-for-php": "^8.13",
        "guzzlehttp/guzzle": "^7.2",
        "hashids/hashids": "^5.0",
        "inspector-apm/inspector-laravel": "^4.8",
        "intervention/image": "^3.7",
        "itmm/m-mart-skin": "^1.0",
        "jeremykenedy/laravel2step": "^3.1",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.5",
        "maatwebsite/excel": "^3.1",
        "pragmarx/countries": "^0.8.2",
        "propaganistas/laravel-phone": "^5.3",
        "protonemedia/laravel-splade": "^1.4",
        "pusher/pusher-php-server": "^7.2",
        "santigarcor/laratrust": "^8.3",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "spatie/laravel-activitylog": "^4.8",
        "torann/geoip": "^3.0",
        "tymon/jwt-auth": "2.1.1",
        "yadahan/laravel-authentication-log": "^1.7",
        "yajra/laravel-datatables-oracle": "^10.11",
        "yaza/laravel-repository-service": "^5.0"
    },
    "require-dev": {
        "laravel/browser-kit-testing": "^7.2",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.5",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "vendor/bin/pest"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
